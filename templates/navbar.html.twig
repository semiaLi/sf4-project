
<div id="screensaver">
	<canvas id="canvas"></canvas>
	<i class="fa fa-lock" id="screen_unlock"></i>
</div>
<div id="modalbox">
	<div class="devoops-modal">
		<div class="devoops-modal-header">
			<div class="modal-header-name">
				<span>Basic table</span>
			</div>
			<div class="box-icons">
				<a class="close-link">
					<i class="fa fa-times"></i>
				</a>
			</div>
		</div>
		<div class="devoops-modal-inner">
		</div>
		<div class="devoops-modal-bottom">
		</div>
	</div>
</div>
<header class="navbar">
	<div class="container-fluid expanded-panel">
		<div class="row">
			<div id="logo" class="col-xs-12 col-sm-2">
				<a href="{{ path('stage_user_homepage') }}">{{ 'title.application'|trans }}</a>
			</div>
			<div id="top-panel" class="col-xs-12 col-sm-10 marge">
				<div class="row">
					<div class="col-xs-8 col-sm-4">
						<a href="#" class="show-sidebar">
						  <i class="fa fa-bars"></i>
						</a>
						<div id="search">
							<input type="text" placeholder="{{ 'navbar.search'|trans }}"/>
							<i class="fa fa-search"></i>
						</div>
					</div>
					<div class="col-xs-4 col-sm-8 top-panel-right">
						<ul class="nav navbar-nav pull-right panel-menu">
							
							<li class="dropdown">
								<a href="#" class="dropdown-toggle account" data-toggle="dropdown">
									<div class="avatar">
										
									</div>
									<i class="fa fa-angle-down pull-right"></i>
									<div class="user-mini pull-right">
										<span class="welcome">{{ 'navbar.welcome'|trans() }},{{app.user.getusername() }}</span>
										<span>{{app.user.getLastLogin()|date("m/d/Y H:i")}}</span>
									</div>
								</a>
								<ul class="dropdown-menu">
									<li>
										<a href="{{ path('fos_user_profile_show') }}">
											<i class="fa fa-user "></i>&nbsp;&nbsp;{{ 'navbar.dropdown.profile'|trans }}
{# 											<span class="hidden-sm text">{{ 'navbar.dropdown.profile'|trans }}</span>#}
										</a>
									</li>
									<li>
										<a href="#">
											<i class="fa fa-cog"></i>&nbsp;&nbsp;{{ 'navbar.dropdown.settings'|trans }}
{# 											<span class="hidden-sm text">{{ 'navbar.dropdown.settings'|trans }}</span>#}
										</a>
									</li>
									{% if is_granted('ROLE_ADMIN') %}
                                    <li>
										<a href="{{ path('fos_user_registration_register') }}">
											<i class="fa fa-plus-circle"></i>&nbsp;&nbsp;{{ 'navbar.dropdown.addUser'|trans }}
{# 											<span class="hidden-sm text">{{ 'navbar.dropdown.addUser'|trans }}</span>#}
										</a>
									</li>
                                    {% endif %}
									<li>
										<a href=" {{ path('fos_user_security_logout') }} ">
											<i class="fa fa-power-off"></i>&nbsp;&nbsp;{{ 'navbar.dropdown.logout'|trans }}
{# 											<span class="hidden-sm text">{{ 'navbar.dropdown.logout'|trans }}</span>#}
										</a>
									</li>
								</ul>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
</header>
<!--End Header-->
